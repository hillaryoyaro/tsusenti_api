#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/root/ultimate_node_stack/node_modules/.pnpm/registry.npmmirror.com+ts-node@10.9.1_@swc+core@1.3.62_@types+node@20.2.5_typescript@5.1.3/node_modules/ts-node/dist/node_modules:/root/ultimate_node_stack/node_modules/.pnpm/registry.npmmirror.com+ts-node@10.9.1_@swc+core@1.3.62_@types+node@20.2.5_typescript@5.1.3/node_modules/ts-node/node_modules:/root/ultimate_node_stack/node_modules/.pnpm/registry.npmmirror.com+ts-node@10.9.1_@swc+core@1.3.62_@types+node@20.2.5_typescript@5.1.3/node_modules:/root/ultimate_node_stack/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/root/ultimate_node_stack/node_modules/.pnpm/registry.npmmirror.com+ts-node@10.9.1_@swc+core@1.3.62_@types+node@20.2.5_typescript@5.1.3/node_modules/ts-node/dist/node_modules:/root/ultimate_node_stack/node_modules/.pnpm/registry.npmmirror.com+ts-node@10.9.1_@swc+core@1.3.62_@types+node@20.2.5_typescript@5.1.3/node_modules/ts-node/node_modules:/root/ultimate_node_stack/node_modules/.pnpm/registry.npmmirror.com+ts-node@10.9.1_@swc+core@1.3.62_@types+node@20.2.5_typescript@5.1.3/node_modules:/root/ultimate_node_stack/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-node/dist/bin-transpile.js" "$@"
else
  exec node  "$basedir/../ts-node/dist/bin-transpile.js" "$@"
fi
